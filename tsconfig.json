{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "lib": ["ESNext"],
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "importsNotUsedAsValues": "error",
    "outDir": "./lib"
  },
  "exclude": ["tests/*"]
}
